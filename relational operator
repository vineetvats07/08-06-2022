/******************************************************************************

program to illustrate relational operator

*******************************************************************************/
#include <stdio.h>
void main()
{
    int num1,num2;
    scanf("%d",&num1);
    scanf("%d",&num2);
    if(num1>num2)
    printf("\nnumber 1 is greater than number 2");
    else if(num1>=num2)
    printf("\nnumber 1 is greater or equal than number 1");
    else
    printf("number 2 is greater than number 1");
    
}

