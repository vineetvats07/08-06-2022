/******************************************************************************
Program to demostrate arithmatic operators
*******************************************************************************/
#include <stdio.h>

int main()
{
   int num1,num2,num3;
   int sum,sub,di;
   scanf("%d",&num1);
   scanf("%d",&num2);
   scanf("%d",&num3);
   sum=num1+num2+num3;
   sub=num1-num2;
   di=num1+num2/num3;
   printf("\nThe sum is %d",sum);
   printf("\nThe subraction is %d",sub);
   printf("\nThe divison is %d",di);
   return 0;
    
}

